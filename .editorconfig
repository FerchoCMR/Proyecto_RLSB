# Editor configuration, see https://editorconfig.org
root = true

[*.yml]
charset = utf-8
indent_style = tab
indent_size = 4
trim_trailing_whitespace = true
max_line_length = 120

[*.md]
max_line_length = off
trim_trailing_whitespace = false

[*.ts]
charset = utf-8
indent_style = tab
indent_size = 4
trim_trailing_whitespace = true
max_line_length = 120
quote_type = single

